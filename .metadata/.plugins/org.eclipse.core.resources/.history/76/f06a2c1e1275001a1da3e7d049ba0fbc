package application;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter the dimensions of the matrix: ");
		int n = sc.nextInt();
		int[][] mat = new int[n][n];
		
		for(int i=0;i<n;i++) {
			for(int j=0; j<n;j++) {
				System.out.print("Enter the values in the matrix rows: ");
				mat[i][j] = sc.nextInt();
			}
		}
		
		sc.close();
	}

}
