package modulo1;

import java.util.Scanner;

public class _59 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int a,b,c;
		System.out.print("Dígite um numero: ");
		a = sc.nextInt();
		System.out.print("\nDígite outro numero: ");
		b = sc.nextInt();
		System.out.print("znDígite outro numero: ");
		c = sc.nextInt();
		if(a > b && a > c) {
			System.out.println("Maior: "+a);
		}else if(b > a && b > c) {
			System.out.println("Maior: "+b);
		}else if(c > a && c > b) {
			System.out.println("Maior: "+c);
	 	
		sc.close();
	}

}
