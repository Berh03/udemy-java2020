package modulo1;

import java.util.Scanner;

public class exFor1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int x;
		System.out.print("Dígite um número entre 1 e 1000: ");
		x = sc.nextInt();

		for (int i = 1; i < x; i+=2) {
			System.out.println("\nNúmeros impares até o número dígitado: "+i);

		}
		sc.close();
	}

}
