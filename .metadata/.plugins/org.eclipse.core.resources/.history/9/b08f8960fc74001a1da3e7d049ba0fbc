package application;

import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		
		List<String> list = new ArrayList<>();
		
		list.add("Kauany");
		list.add("Bernardo");
		list.add("Maria");
		list.add("Guilherme");
		list.add(2, "Thiago");
		
		System.out.println(list.size());
		
		list.remove(5);
		
		System.out.println(list.size());
		
		for (String x : list) {
			System.out.println(x);
		}
		
	}

}
